import java.util.Scanner;
class  Reverse_Uptolow
{
	public static void main(String[] args) 
	{
		Scanner sc =new Scanner(System.in);
		System.out.println("Enter the String");
		String s= sc.next();
		String rev="";
		for (int i=s.length()-1;i>=0;i-- )
		{
			char ch=s.charAt(i);
			if (ch>='a'&&ch<='z')    
			{
				char ch1=(char)(ch-32);
				rev+=ch1;
				
			}
			if(ch>='A'&&ch<='Z'){
				char ch2=(char)(ch+32);
				rev=rev+ch2;
			
		    }
			
		}

		System.out.println(rev);
		
	}
}
